{
  "expo": {
    "name": "goit-react-native-hw",
    "slug": "goit-react-native-hw",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pavel1975.goitreactnativehw"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "process.env.EXPO_PUBLIC_API_URL"
        }
      },
      "package": "com.pavel1975.goitreactnativehw"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(goit-react-native-hw) to access your camera."
        }
      ]
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d1274a3c-6051-4fa9-a973-555696eb23cf"
      }
    }
  }
}
